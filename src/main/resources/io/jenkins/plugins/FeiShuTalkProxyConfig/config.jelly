<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core"
         xmlns:f="/lib/form">

    <f:entry title="代理类型">
        <f:enum field="type" default="DIRECT">
            ${it.name()}
        </f:enum>
    </f:entry>
    <f:entry title="代理服务" field="host">
        <f:textbox/>
